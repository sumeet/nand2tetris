/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // TODO: should i use fewer parts for this and just built it out of nand gates?
    Or(a=a, b=b, out=orab);
    Nand(a=a, b=b, out=nandab);
    And(a=orab, b=nandab, out=out);
}
